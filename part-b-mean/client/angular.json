{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "client": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "scripts": [
              "src/assets/js/ui/common/accordion-ui.js",
              "src/assets/js/ui/common/card-carousel-drag.js",
              "src/assets/js/ui/common/categories-intro-slider-height.js",
              "src/assets/js/ui/common/intro-slider.js",
              "src/assets/js/ui/common/categories-intro-slider.js",

              "src/assets/js/ui/common/course-items-reveal.js",
              "src/assets/js/ui/common/proposed-courses-carousel.js",
              "src/assets/js/ui/common/reveal.js",
              "src/assets/js/ui/common/menu-filters.js",
              "src/assets/js/ui/common/modal.js"
            ],
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/reset.css",
              "src/assets/css/theme.css",
              "src/assets/css/components.css",
              "src/assets/css/layout.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "client:build:production"
            },
            "development": {
              "buildTarget": "client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    }
  }
}